<%= simple_form_for(@tutorial) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :package_id %>
    <%= f.input :price %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :length %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
